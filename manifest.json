{
    "manifest_version": 2,
    "name": "Geniusify",
    "short_name": "Geniusify",
    "description": "Find out every detail about a song",
    "version": "1.0",
    "icons": {
        "48": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab",
    	"storage",
    	"identity",
    	"https://api.genius.com/"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
